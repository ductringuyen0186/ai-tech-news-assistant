{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "vite",
  "env": {
    "VITE_API_BASE_URL": "@api_base_url"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
